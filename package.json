{
    "name": "vite-react-typescript-starter",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
      "dev": "vite",
      "lint": "eslint .",
      "preview": "vite preview",
      "dev:server": "tsx watch src/server.ts",
      "dev:start": "concurrently \"npm:dev\" \"npm:dev:server\"",
      "build": "npx prisma generate && npx prisma migrate resolve --applied 0_init && npx prisma migrate deploy && vite build",
      "start": "tsx src/server.ts"
    },
    "dependencies": {
        "@prisma/client": "^6.15.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "lucide-react": "^0.344.0",
        "prisma": "^6.15.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.18",
        "concurrently": "^9.2.1",
        "eslint": "^9.9.1",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.11",
        "globals": "^15.9.0",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "tsx": "^4.20.5",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.3.0",
        "vite": "^5.4.2"
    }
}
